<script>
var drawTimer=function(percent)
{
  percent=percent || 0;
  stageC=$('#home-slider-pagination .selected canvas');
  if (stageC.length)
  {
    stage=stageC[0].getContext('2d');
    stage.save();
    stage.clearRect(0,0,42,42);
            if(percent)
            {
              stage.beginPath();
              stage.arc(21,21,21, 1.5*Math.PI, (percent-25)/50*Math.PI, false);
              stage.lineTo(21,21)        
              stage.closePath();
              stage.lineWidth = 0;
              stage.fillStyle = "#8ED6FF";
              stage.fill();
            }
    stage.restore();        
  }
}
</script>

